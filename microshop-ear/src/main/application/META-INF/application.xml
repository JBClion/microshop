<?xml version="1.0"?>
<!-- This is the application.xml file intended for use with gradle, if you find it in your maven project, then that's a problem most likely. -->
<application xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_6.xsd" version="6">
  <display-name>microshop</display-name>
  <module>
    <module>
      <web>
        <web-uri>catalog-1.0</web-uri>
        <context-root>catalog</context-root>
      </web>
    </module>
    <web>
      <web-uri>cart-1.0</web-uri>
      <context-root>cart</context-root>
    </web>
  </module>
  <module>
    <web>
      <web-uri>order-1.0</web-uri>
      <context-root>order</context-root>
    </web>
  </module>
  <library-directory>lib</library-directory>
</application>
